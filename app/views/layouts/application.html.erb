<!DOCTYPE html>
<html>
<head>
  <title>Squeeze</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <%= favicon_link_tag 'menu/logo.png' %>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= stylesheet_link_tag '//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>" style="margin-top:50px;">
        <%= value %>
        <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
  <% end %>
  <div class="container" style="width:95%;">
    <div class="row">
      <div id="menu-desktop">
        <%= render 'layouts/desktopmenu' %>
      </div>
      <div id="menu-mobile">
        <%= render 'layouts/mobilemenu' %>
      </div>
      <div class="col-md-10 col-sm-10 col-md-offset-2 col-sm-offset-2" style="padding-top:50px;padding-bottom:200px;">
        <%= render 'layouts/topmenu' %>
        <%= yield %>
      </div>
    </div>
  </div>
  <i class="scrollToTop angle double up icon" style="font-size:48px;"></i>
  <%= render 'users/contract' %>
</body>
</html>
